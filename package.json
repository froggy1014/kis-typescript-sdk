{
	"name": "kis-typescript-sdk",
	"version": "0.0.3",
	"description": "Korea Investment Securities API TypeScript SDK with OpenAPI documentation",
	"author": "froggy1014",
	"repository": "froggy1014/kis-typescript-sdk",
	"license": "MIT",
	"main": "./dist/package/index.js",
	"private": false,
	"files": [
		"dist/package/"
	],
	"scripts": {
		"build": "npm run generate && tsc && tsdown",
		"start:dev": "node --import=tsx --watch src/index.ts",
		"start:prod": "node dist/index.js",
		"check": "biome check --write",
		"generate:openapi": "node --import=tsx scripts/generate-open-api.ts",
		"generate:tr-mappings": "node --import=tsx scripts/generate-tr-id-mappings.ts",
		"generate:kis-api": "orval --config orval.config.ts",
		"generate": "npm run generate:openapi && npm run generate:tr-mappings && npm run generate:kis-api"
	},
	"dependencies": {
		"@asteasolutions/zod-to-openapi": "7.3.4",
		"axios": "^1.12.2",
		"cors": "2.8.5",
		"express": "5.1.0",
		"helmet": "8.1.0",
		"swagger-ui-express": "5.0.1",
		"zod": "3.25.76"
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.4",
		"@types/cors": "2.8.19",
		"@types/express": "5.0.3",
		"@types/swagger-ui-express": "4.1.8",
		"orval": "^7.11.2",
		"tsdown": "^0.15.4",
		"tsx": "4.20.4",
		"typescript": "5.9.2",
		"vite-tsconfig-paths": "5.1.4",
		"vitest": "3.2.4"
	},
	"packageManager": "pnpm@10.14.0",
	"module": "./dist/package/index.mjs",
	"types": "./dist/package/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/package/index.mjs",
			"require": "./dist/package/index.js"
		},
		"./package.json": "./package.json"
	}
}
